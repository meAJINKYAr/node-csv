<%- include('base') %>
<div class="container m-3 p-3">
    <div class=""><b>Total rows: <%= lines-1 %></b></div>
    <table class="table table-hover">
        <thead class="thead-dark">
            <tr>
                <th><%= data[0][0] %></th>
                <th><%= data[0][1] %></th>
                <th><%= data[0][2] %></th>
            </tr>
        </thead>
        <tbody>
            <% for(let i=1;i< data.length;i++){ %>
                <tr>
                    <td><%= data[i][0] %></td>
                    <td><%= data[i][1] %></td>
                    <td><%= data[i][2] %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>